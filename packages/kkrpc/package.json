{
	"name": "kkrpc",
	"version": "0.4.0",
	"description": "TypeScript-first RPC library for seamless bi-directional communication between processes, workers, and contexts. Cross-runtime compatible with Node.js, Deno, Bun, and browsers.",
	"type": "module",
	"keywords": [
		"rpc",
		"typescript",
		"remote-procedure-call",
		"type-safe",
		"bidirectional",
		"cross-runtime",
		"nodejs",
		"deno",
		"bun",
		"browser",
		"web-worker",
		"chrome-extension",
		"tauri",
		"ipc",
		"inter-process-communication",
		"extension-system",
		"plugin-architecture",
		"microservices",
		"worker-communication",
		"cross-context",
		"stdio",
		"http",
		"websocket",
		"postmessage"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/kunkunsh/kkrpc.git"
	},
	"scripts": {
		"prepare": "bun run scripts/prepare.ts",
		"test": "bun run scripts/test.ts",
		"posttest": "verify-package-export verify",
		"docs": "typedoc --out docs",
		"build": "bun run build.ts",
		"dev": "tsdown --watch"
	},
	"main": "./dist/mod.js",
	"types": "./dist/mod.d.ts",
	"exports": {
		".": {
			"types": {
				"import": "./dist/mod.d.ts",
				"require": "./dist/mod.cjs",
				"default": "./dist/mod.js"
			},
			"import": "./dist/mod.js",
			"require": "./dist/mod.cjs"
		},
		"./*": "./src/*",
		"./browser": {
			"types": {
				"import": "./dist/browser-mod.d.ts",
				"require": "./dist/browser-mod.cjs",
				"default": "./dist/browser-mod.js"
			},
			"import": "./dist/browser-mod.js",
			"require": "./dist/browser-mod.cjs"
		},
		"./http": {
			"types": {
				"import": "./dist/http.d.ts",
				"require": "./dist/http.cjs",
				"default": "./dist/http.js"
			},
			"import": "./dist/http.js",
			"require": "./dist/http.cjs"
		},
		"./deno": {
			"types": {
				"import": "./dist/deno-mod.d.ts",
				"require": "./dist/deno-mod.cjs",
				"default": "./dist/deno-mod.js"
			},
			"import": "./dist/deno-mod.js",
			"require": "./dist/deno-mod.cjs"
		},
		"./chrome-extension": {
			"types": {
				"import": "./dist/chrome-extension.d.ts",
				"require": "./dist/chrome-extension.cjs",
				"default": "./dist/chrome-extension.js"
			},
			"import": "./dist/chrome-extension.js",
			"require": "./dist/chrome-extension.cjs"
		},
		"./socketio": {
			"types": {
				"import": "./dist/socketio.d.ts",
				"require": "./dist/socketio.cjs",
				"default": "./dist/socketio.js"
			},
			"import": "./dist/socketio.js",
			"require": "./dist/socketio.cjs"
		}
	},
	"devDependencies": {
		"@types/bun": "^1.2.20",
		"@types/chrome": "^0.1.4",
		"@types/node": "^24.3.0",
		"@types/ws": "^8.18.0",
		"tslib": "^2.8.1",
		"tsdown": "^0.15.9",
		"typedoc": "^0.28.10",
		"verify-package-export": "^0.0.3"
	},
	"peerDependencies": {
		"typescript": "^5.0.0",
		"socket.io": "^4.0.0",
		"socket.io-client": "^4.0.0"
	},
	"peerDependenciesMeta": {
		"socket.io": {
			"optional": true
		},
		"socket.io-client": {
			"optional": true
		}
	},
	"dependencies": {
		"@tauri-apps/plugin-shell": "^2.2.0",
		"socket.io": "^4.8.1",
		"socket.io-client": "^4.8.1",
		"superjson": "^2.2.2",
		"ws": "^8.18.1"
	},
	"files": [
		"dist"
	]
}
